<launch>
<!-- Launch file for Livox Horizon LiDAR -->

	<!--/home/maple/study2/merge_ws3/src/merge/src/point_lio/config/mid360.yaml-->
	<arg name="rviz" default="true" />
	<arg name="node_exit_timeout" default="10" />

	<node pkg="merge" type="merge_node" name="merge_node" output="screen" required="true" respawn="false">
	<rosparam command="load" file="$(find merge)/src/point_lio/config/mid360.yaml" />
	<param name="use_imu_as_input" type="bool" value="1"/> <!--change to 1 to use IMU as input of Point-LIO-->
	<param name="prop_at_freq_of_imu" type="bool" value="1"/>
	<param name="check_satu" type="bool" value="0"/>
	<param name="init_map_size" type="int" value="10"/>
	<param name="point_filter_num" type="int" value="1"/> <!--1, 3--> 
	<param name="space_down_sample" type="bool" value="1" />  
	<param name="filter_size_surf" type="double" value="0.5" /> <!--0.5, 0.3, 0.2, 0.15, 0.1--> 
	<param name="filter_size_map" type="double" value="0.5" /> <!--0.5, 0.3, 0.15, 0.1-->
	<param name="ivox_nearby_type" type="int" value="6" /> <!--0.5, 0.3, 0.15, 0.1-->
	<param name="runtime_pos_log_enable" type="bool" value="0" /> <!--1-->
	</node>

	
	<!--
	<node pkg="serial_node" type="serial_node" name="serial_node" output="screen"/>

	<node pkg="debug" type="pub_serial_node" name="pub_serial_node" output="screen"/>
	<node pkg="debug" type="protected_node" name="protected_node" output="screen">
	<param name="bash_path" type="string" value="$(find debug)/bash/serial.sh"/>
	</node>
	-->
	<group if="$(arg rviz)">
	<node launch-prefix="nice" pkg="rviz" type="rviz" name="rviz" args="-d $(find merge)/rviz_cfg/loam_livox.rviz" output="screen"/>
	</group>
	
	<!--<include file="$(find yolopkg)/launch/yolo.launch" />-->
	<param name="/roslaunch/child_terminate_timeout" value="$(arg node_exit_timeout)" type="int"/>
	launch-prefix="gdb -ex run --args"

</launch>